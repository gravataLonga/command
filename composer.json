    "require": {
        "php": "~7.2",
        "gravatalonga/console": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Commands\\": "Commands/"
        }
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
