{
    "name": "gravatalonga/command",
    "type": "library",
    "description": "Ready project to create commands",
    "keywords": [],
    "homepage": "https://github.com/gravatalonga/command",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Fontes",
            "email": "jonathan.alexey16@gmail.com",
            "homepage": "https://jonathan.pt",
            "role": "Developer"
        }
